# Staging Environment Variables
# For testing before production deployment

# API Configuration - Staging server
VITE_API_URL=https://staging.yourdomain.com/api
# Or with staging IP: https://192.168.1.101:3001/api

# Application Configuration
VITE_APP_NAME=SSM Technologies (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_ENV=staging

# Security Configuration
VITE_RECAPTCHA_SITE_KEY=your-staging-recaptcha-site-key-here

# Analytics (disabled for staging)
VITE_GA_TRACKING_ID=
VITE_ENABLE_ANALYTICS=false

# Feature Flags
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_DEBUG_MODE=true

# CDN Configuration (if using)
VITE_CDN_URL=https://staging-cdn.yourdomain.com

# Social Media Links (staging/test accounts)
VITE_FACEBOOK_URL=https://facebook.com/ssmtechnologies-staging
VITE_TWITTER_URL=https://twitter.com/ssmtech-staging
VITE_LINKEDIN_URL=https://linkedin.com/company/ssmtechnologies-staging